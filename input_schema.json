{
  "schemaVersion": 1,
  "title": "SearchLeads Enrichment",
  "description": "Enrich leads from Apollo.io search results using SearchLeads API",
  "type": "object",
  "required": ["apolloLink", "noOfLeads", "fileName"],
  "properties": {
    "apolloLink": {
      "type": "string",
      "title": "Apollo Search Link",
      "description": "The Apollo.io search URL containing the leads you want to enrich. Copy the URL from your Apollo search results page.",
      "editor": "textfield",
      "example": "https://app.apollo.io/#/people?page=1&sortAscending=false&sortByField=recommendations_score&personLocations[]=United%20States"
    },
    "noOfLeads": {
      "type": "integer",
      "title": "Number of Leads",
      "description": "The maximum number of leads to enrich from the Apollo search results. Must be in multiples of 1000 (1000, 2000, 3000, etc.) up to 50,000.",
      "default": 1000,
      "editor": "select",
      "enum": [1000, 2000, 3000, 4000, 5000, 6000, 7000, 8000, 9000, 10000, 11000, 12000, 13000, 14000, 15000, 16000, 17000, 18000, 19000, 20000, 21000, 22000, 23000, 24000, 25000, 26000, 27000, 28000, 29000, 30000, 31000, 32000, 33000, 34000, 35000, 36000, 37000, 38000, 39000, 40000, 41000, 42000, 43000, 44000, 45000, 46000, 47000, 48000, 49000, 50000],
      "enumTitles": ["1,000", "2,000", "3,000", "4,000", "5,000", "6,000", "7,000", "8,000", "9,000", "10,000", "11,000", "12,000", "13,000", "14,000", "15,000", "16,000", "17,000", "18,000", "19,000", "20,000", "21,000", "22,000", "23,000", "24,000", "25,000", "26,000", "27,000", "28,000", "29,000", "30,000", "31,000", "32,000", "33,000", "34,000", "35,000", "36,000", "37,000", "38,000", "39,000", "40,000", "41,000", "42,000", "43,000", "44,000", "45,000", "46,000", "47,000", "48,000", "49,000", "50,000"]
    },
    "fileName": {
      "type": "string",
      "title": "File Name",
      "description": "A descriptive name for your enriched leads file (without extension).",
      "editor": "textfield",
      "example": "my_enriched_leads"
    }
  }
}

